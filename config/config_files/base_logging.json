{
    "level": "INFO",
    "console": {
      "enabled": true,
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "file": {
      "enabled": true,
      "level": "DEBUG",
